-    # If it is mounted
-    if [ $IS_MOUNTED_TEST_RESULT == "0" ];
-    then
-        # Then unmount it
-        umount /dev/${BLOCK_DEVICE}
-
-    else
-        # Device is not mounted.  Test if the mount point exists.
